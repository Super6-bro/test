//初始化名字
git config --global user.name "Super6-bro"

//初始化邮箱
git config --global user.email "1871257219@qq.com"

//设置当前文件夹为仓库
git init

//查看库中的文件
git status

//提交文件至暂存区
git add 文件全称

//提交文件至仓库
git commit -m提交文件描述

//删除库中文件
git rm 文件名

//将本地仓库提交到远程
git push

//克隆远程仓库
git clone 仓库地址

//查看修改日志
git log

//返回上一版本
git reset --hard 版本号

//获取版本号
git reflog

//撤销修改
git restore -- 文件名

